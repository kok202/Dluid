                                         </TextField>
                                     </children>
                                 </AnchorPane>

                                 <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="3">
                                     <children>
                                         <TextField fx:id="textFieldStrideSizeX" promptText="5" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                         </TextField>
                                     </children>
                                 </AnchorPane>
                                 <AnchorPane GridPane.columnIndex="2" GridPane.rowIndex="3">
                                     <children>
                                         <TextField fx:id="textFieldStrideSizeY" promptText="5" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                         </TextField>
                                     </children>
                                 </AnchorPane>

                                 <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="4">
