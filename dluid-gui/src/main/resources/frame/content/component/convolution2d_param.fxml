                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                             <children>
                                 <Label fx:id="labelInputSize" text="Input size" GridPane.rowIndex="1" />
                                 <Label fx:id="labelKernelSize" text="Kernel size" GridPane.rowIndex="2" />
                                <Label fx:id="labelStrideSize" text="Stride size" GridPane.rowIndex="3" />
                                <Label fx:id="labelPaddingSize" text="Padding size" GridPane.rowIndex="4" />
                                 <Label fx:id="labelOutputSize" text="Output size" GridPane.rowIndex="5" />
                                 <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="0">
                                     <children>
                                         <Label fx:id="labelWidth" alignment="CENTER" text="Width" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                     </children>
                                 </AnchorPane>
                                 <AnchorPane GridPane.columnIndex="2" GridPane.rowIndex="0">
                                     <children>
                                         <Label fx:id="labelHeight" alignment="CENTER" text="Height" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                     </children>
                                 </AnchorPane>

                                 <!--==================================================================================Value Field==================================================================================-->

                                 <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="1">
                                     <children>
                                         <TextField fx:id="textFieldInputSizeX" promptText="5" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                         </TextField>
                                     </children>
                                 </AnchorPane>
                                 <AnchorPane GridPane.columnIndex="2" GridPane.rowIndex="1">
                                     <children>
                                         <TextField fx:id="textFieldInputSizeY" promptText="5" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                         </TextField>
                                     </children>
                                 </AnchorPane>

                                 <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="2">
                                     <children>
                                         <TextField fx:id="textFieldKernelSizeX" promptText="5" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                         </TextField>
                                     </children>
                                 </AnchorPane>
                                 <AnchorPane GridPane.columnIndex="2" GridPane.rowIndex="2">
                                     <children>
