                                         </TextField>
                                     </children>
                                 </AnchorPane>

                                 <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="5">
                                     <children>
                                         <TextField fx:id="textFieldOutputSizeValueX" promptText="5" disable="true" editable="false" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                     </children>
                                 </AnchorPane>
                                 <AnchorPane GridPane.columnIndex="2" GridPane.rowIndex="5">
                                     <children>
                                         <TextField fx:id="textFieldOutputSizeValueY" promptText="5" disable="true" editable="false" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                     </children>
                                 </AnchorPane>
