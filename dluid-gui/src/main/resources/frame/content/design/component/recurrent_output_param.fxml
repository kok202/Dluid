<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="itself" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TitledPane fx:id="titledPane" animated="false" text="Layer parameter setting" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0.0">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0">
                    <children>
                        <!-- start content-->
                        <GridPane hgap="5.0" vgap="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="150" minWidth="130" prefWidth="140" />
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="80" prefWidth="120" />
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="80" prefWidth="120" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                             <children>
                                 <Label fx:id="labelInputSize" text="Input size" GridPane.rowIndex="1" />
                                 <Label fx:id="labelOutputSize" text="Output size" GridPane.rowIndex="2" />
                                 <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="0">
                                     <children>
                                         <Label fx:id="labelInputOutputX" alignment="CENTER" text="Width" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                     </children>
                                 </AnchorPane>
                                 <AnchorPane GridPane.columnIndex="2" GridPane.rowIndex="0">
                                     <children>
                                         <Label fx:id="labelInputOutputY" alignment="CENTER" text="Height" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                     </children>
                                 </AnchorPane>
                                 <Label fx:id="labelLossFunction" text="Loss function" GridPane.rowIndex="3" />

                                 <!--==================================================================================Value Field==================================================================================-->
                                 <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="1">
                                     <children>
                                         <TextField fx:id="textFieldInputSizeX" promptText="5" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                         </TextField>
                                     </children>
                                 </AnchorPane>
                                 <AnchorPane GridPane.columnIndex="2" GridPane.rowIndex="1">
                                     <children>
                                         <TextField fx:id="textFieldInputSizeY" promptText="5" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                         </TextField>
                                     </children>
                                 </AnchorPane>
                                 <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="2">
                                     <children>
                                         <TextField fx:id="textFieldOutputSizeX" promptText="5" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                     </children>
                                 </AnchorPane>
                                 <AnchorPane GridPane.columnIndex="2" GridPane.rowIndex="2">
                                     <children>
                                         <TextField fx:id="textFieldOutputSizeY" disable="true" editable="false" promptText="5" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                         </TextField>
                                     </children>
                                 </AnchorPane>
                                 <AnchorPane GridPane.columnIndex="1"  GridPane.rowIndex="3" GridPane.columnSpan="2">
                                     <children>
                                         <MenuButton fx:id="menuButtonLossFunction" styleClass="customMenuButton" mnemonicParsing="false" text="" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                             <items>
                                             </items>
                                         </MenuButton>
                                     </children>
                                 </AnchorPane>

                             </children>
                        </GridPane>
                        <!-- end content-->
                    </children>
                </AnchorPane>
            </content>
        </TitledPane>
    </children>
</AnchorPane>
