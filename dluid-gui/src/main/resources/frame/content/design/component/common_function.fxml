<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXSlider?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="itself" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TitledPane fx:id="titledPane" animated="false" text="Layer function setting" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0.0">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0">
                    <children>
                        <!-- start content-->
                        <GridPane vgap="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="150" minWidth="130" prefWidth="140" />
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="80" prefWidth="240" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                            <children>
                                <Label fx:id="labelWeightInitializer" text="Weight Initializer" GridPane.rowIndex="0" />
                                <Label fx:id="labelBiasInitializer" text="Bias Initializer" GridPane.rowIndex="1" />
                                <Label fx:id="labelActivationFunction" text="Activation function" GridPane.rowIndex="2" />
                                <Label fx:id="labelDropout" text="Dropout ratio" GridPane.rowIndex="3" />
                                <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="0">
                                    <children>
                                        <MenuButton fx:id="menuButtonWeightInit" styleClass="customMenuButton" mnemonicParsing="false" text="" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            <items>
                                            </items>
                                        </MenuButton>
                                    </children>
                                </AnchorPane>
                                <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="1">
                                    <children>
                                        <MenuButton fx:id="menuButtonBiasInit" styleClass="customMenuButton" mnemonicParsing="false" text="" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            <items>
                                            </items>
                                        </MenuButton>
                                    </children>
                                </AnchorPane>
                                <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="2">
                                    <children>
                                        <MenuButton fx:id="menuButtonActivationFunction" styleClass="customMenuButton" mnemonicParsing="false" text="" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            <items>
                                            </items>
                                        </MenuButton>
                                    </children>
                                </AnchorPane>
                                <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="3">
                                    <children>
                                        <JFXSlider fx:id="sliderDropout" min="0.1" max="1.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                        </JFXSlider>
                                    </children>
                                </AnchorPane>
                            </children>
                        </GridPane>
                        <!-- end content-->
                    </children>
                </AnchorPane>
            </content>
        </TitledPane>
    </children>
</AnchorPane>
